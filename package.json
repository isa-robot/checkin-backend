    "tsyringe": "4.2.0",
    "typeorm": "^0.2.24",
    "uuid": "7.0.3",
    "uuidv4": "6.0.7",
    "yup": "0.28.3"
  },
  "devDependencies": {
    "@babel/preset-env": "7.9.5",
    "@types/bcryptjs": "2.4.2",
    "@types/cors": "2.8.6",
    "@types/express": "4.17.6",
    "@types/jest": "25.2.1",
    "@types/jsonwebtoken": "8.3.9",
    "@types/multer": "1.4.3",
    "@types/nodemailer": "6.4.0",
    "@types/swagger-ui-express": "4.1.2",
    "@types/yup": "0.28.0",
    "babel-jest": "25.4.0",

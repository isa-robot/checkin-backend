    "typescript": "3.8.3"
  },
  "_moduleAliases": {
    "@establishments": "src/modules/establishments/",
    "@protocols": "src/modules/protocols/",
    "@users": "src/modules/users/",
    "@notification": "src/modules/notification/",
    "@security": "src/modules/security/",
    "@modules": "src/modules/",
    "@config": "src/config/",
    "@shared": "src/shared/",
    "@docs": "src/docs/",
    "@errors": "src/shared/errors/",
    "@routes": "src/shared/infra/http/routes/"
  }

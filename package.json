{
  "name": "isa-backend",
  "version": "1.12.0",
  "main": "server.js",
  "repository": "https://github.com/Qualis-Infectologia/ISA-Backend.git",
  "scripts": {
    "build": "tsc",
    "cpupload": "cp -R src/modules/signature/uploads dist/modules/signature",
    "postbuild": "cp -R src/shared/infra/jobs/views dist/shared/infra/jobs/views && cp -R node_modules dist/node_modules && cp infra/builder/package.json dist/package.json && cp infra/builder/ormconfig.js dist/shared/infra/typeorm/ormconfig.js && yarn cpupload",
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpileOnly --ignore-watch node_modules src/shared/infra/http/server.ts",
    "start": "nodemon src/shared/infra/http/server.ts",
    "test": "yarn jest --passWithNoTests",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "install:clean": "rm -rf node_modules/ && rm -rf yarn.lock && yarn install",
    "seed:config": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed",
    "schema:drop": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:drop",
    "schema:sync": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:sync",
    "cleanup": "rm -rf node_modules dist package-lock.json yarn.lock",
    "preproduction": "npm run cleanup && npm i",
    "production": "npm run build"
  },
  "resolutions": {
    "tslib": "^2.2.0"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.13.0",
    "@types/agenda": "2.0.8",
    "@types/csvtojson": "1.1.5",
    "@types/express-session": "1.17.0",
    "@types/faker": "4.1.11",
    "@types/node": "^16.7.8",
    "@zenvia/sdk": "1.2.0",
    "@zenvia/zenvia-sms-core": "0.1.0",
    "Faker": "0.7.2",
    "agenda": "3.1.0",
    "aws-sdk": "2.689.0",
    "axios": "^0.21.1",
    "bcryptjs": "2.4.3",
    "body-parser": "1.19.0",
    "buffer-replace": "^1.0.0",
    "cors": "2.8.5",
    "csvtojson": "2.0.10",
    "date-fns": "2.14.0",
    "docx-pdf": "0.0.1",
    "docxtemplater": "^3.23.2",
    "dotenv": "8.2.0",
    "e": "0.0.4",
    "express": "4.17.1",
    "express-async-errors": "3.1.1",
    "express-list-endpoints": "4.0.1",
    "express-session": "1.17.1",
    "handlebars": "4.7.6",
    "json-2-csv": "3.7.8",
    "jsonwebtoken": "8.5.1",
    "keycloak-admin-client": "0.12.0",
    "keycloak-connect": "10.0.2",
    "keycloak-request-token": "0.1.0",
    "libreoffice-convert": "^1.3.2",
    "log4js": "6.3.0",
    "log4js-node-mongodb": "2.2.1",
    "module-alias": "2.2.2",
    "moment": "2.24.0",
    "mongoose": "^5.12.10",
    "multer": "1.4.2",
    "nodemailer": "6.4.8",
    "nodemailer-express-handlebars": "4.0.0",
    "nodemailer-ses-transport": "1.5.1",
    "nodemon": "2.0.4",
